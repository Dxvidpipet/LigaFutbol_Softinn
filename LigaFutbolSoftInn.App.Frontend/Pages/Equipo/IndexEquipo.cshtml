@page
@model LigaFutbolSoftInn.App.Frontend.Pages.IndexEquipoModel
@{
}
<h1>Listado Equipos</h1>
<table class="table">
    <thead>
        <tr>
            <th scope="col">Id Equipo</th>
            <th scope="col">Nombre Equipo</th>
            <th scope="col">Municipio Id Municipio</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var equipo in Model.equipos)
        {
            <tr>
                <td>@equipo.IdEquipo</td>
                <td>@equipo.NombreEquipo</td>
                <td>@equipo.Municipio</td>
            
            </tr>
             <td>
                    <a class="btn btn-primary" asp-page="./DetailsEquipo" asp-route-id="@equipo.IdEquipo">
                        Detalle
                    </a>
                </td>
        }
    </tbody>
</table>