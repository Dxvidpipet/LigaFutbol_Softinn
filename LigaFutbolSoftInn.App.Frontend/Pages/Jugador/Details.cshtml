@page
@model LigaFutbolSoftInn.App.Frontend.Pages.DetailsJugadorModel
@{
}
<h1>Detalles del Jugador</h1>
<table class="table table-dark">
    <tr>
        <th scope="col">Id Jugador</th>
        <td>@Model.jugador.IdJugador</td>
    </tr>
    <tr>
        <th scope="col">Nombre Jugador</th>
        <td>@Model.jugador.NombreJugador</td>
    </tr>
    <tr>
        <th scope="col">Numero</th>
        <td>@Model.jugador.NumeroJugador</td>
    </tr>
    <tr>
        <th scope="col">Posicion Jugador</th>
        <td>@Model.jugador.PosicionJugador</td>
    </tr>
    <tr>
        <th scope="col">Equipo</th>
        <td>@Model.jugador.Equipo</td>
    </tr>
</table>
<a class="btn btn-primary" asp-page="./Index">Volver</a>
