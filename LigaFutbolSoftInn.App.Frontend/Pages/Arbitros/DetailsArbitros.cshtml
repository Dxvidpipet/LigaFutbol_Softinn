@page
@model LigaFutbolSoftInn.App.Frontend.Pages.DetailsArbitrosModel
@{
}
<h1>Detalles del Árbitro</h1>
<table class="table table-dark">
    <tr>
        <th scope="col">ID</th>
        <td>@Model.arbitro.IdArbitro</td>
    </tr>
    <tr>
        <th scope="col">Nombre</th>
        <td>@Model.arbitro.NombreArbitro</td>
    </tr>
    <tr>
        <th scope="col">Documento</th>
        <td>@Model.arbitro.DocumentoArbitro</td>
    </tr>
    <tr>
        <th scope="col">Telefono</th>
        <td>@Model.arbitro.TelefonoArbitro</td>
    </tr>
    <tr>
        <th scope="col">Colegio</th>
        <td>@Model.arbitro.ColegioArbitro</td>
    </tr>
</table>
<a class="btn btn-primary" asp-page="./IndexArbitro">Volver</a>
